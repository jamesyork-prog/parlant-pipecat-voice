{
  "dashboard": {
    "id": null,
    "title": "Async Webhook Orchestration",
    "tags": ["webhook", "orchestration", "async"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Webhook Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(webhook_response_time_ms_bucket[5m]))",
            "legendFormat": "p99"
          },
          {
            "expr": "histogram_quantile(0.95, rate(webhook_response_time_ms_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(webhook_response_time_ms_bucket[5m]))",
            "legendFormat": "p50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Task Processing Throughput",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(webhook_tasks_total[5m]) * 60",
            "legendFormat": "Tasks/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "webhook_queue_depth",
            "legendFormat": "Queue Depth"
          }
        ],
        "yAxes": [
          {
            "label": "Tasks",
            "min": 0
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [100], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "last"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "Queue Depth Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Task Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(webhook_task_completions_total{status=\"success\"}[5m]) / rate(webhook_tasks_total[5m]) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "webhook_active_workers",
            "legendFormat": "Workers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 6,
        "title": "Dead Letter Queue Size",
        "type": "stat",
        "targets": [
          {
            "expr": "webhook_dead_letter_queue_size",
            "legendFormat": "DLQ Size"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 7,
        "title": "Processing Time by Path",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(task_processing_time_seconds_bucket{path=\"fast_path\"}[5m]))",
            "legendFormat": "Fast Path p95"
          },
          {
            "expr": "histogram_quantile(0.95, rate(task_processing_time_seconds_bucket{path=\"llm_pipeline\"}[5m]))",
            "legendFormat": "LLM Pipeline p95"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Error Rate by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(webhook_task_failures_total{error_type=\"transient\"}[5m])",
            "legendFormat": "Transient Errors"
          },
          {
            "expr": "rate(webhook_task_failures_total{error_type=\"permanent\"}[5m])",
            "legendFormat": "Permanent Errors"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}